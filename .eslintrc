{
    "extends": [
        "airbnb",
        "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
        "jsx-a11y"
    ],
    "rules": {
        "semi": 0,
        "indent": ["error", 4],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "import/prefer-default-export": 0,
        "react/jsx-indent" : ["error", 4],
        "object-curly-spacing": 0,
        "array-bracket-spacing": ["error", "never"]
    }
}
